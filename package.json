		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"puppeteer": "^22.15.0",
		"svelte": "^5.15.0",
		"svelte-check": "^4.1.1",
		"tailwindcss": "^4.0.10",
		"typescript": "^5.6.2",
		"vite": "^5.4.11",
		"vite-plugin-glsl": "^1.3.0",
		"vitest": "^1.6.0"
